{
    "name": "shelfwood/project-prompt-generator",
    "type": "project",
    "description": "Generates a prompt for a given Laravel project you are viewing in the CLI",
    "keywords": [
        "framework",
        "laravel",
        "GPT"
    ],
    "homepage": "https://github.com/j-shelfwood/project-prompt-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Joris Schelfhout",
            "email": "joris@shelfwood.co"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/collections": "^10.0",
        "illuminate/database": "^10.0",
        "laravel-zero/phar-updater": "^1.3",
        "nunomaduro/termwind": "^1.15",
        "nyholm/psr7": "^1.5",
        "openai-php/client": "^0.4.1",
        "symfony/http-client": "^6.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.5",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^1.22.3",
        "laravel-zero/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/prompt"
    ]
}
